        .header {
            border-bottom: 2px solid #10b981; /* verde */
            padding-bottom: 10px;
            margin-bottom: 12px;
        }
        .header-inner {
            width: 100%;
        }
        .logo-cell {
            width: 70px;
            vertical-align: middle;
            text-align: left;
        }
        .logo-cell img {
            max-width: 65px;
            max-height: 65px;
        }
        .company-cell {
            text-align: left;
            vertical-align: middle;
            padding-left: 8px;
        }
        .company-name {
            font-size: 16px;
            font-weight: bold;
            color: #065f46;
        }
        .company-subtitle {
            font-size: 11px;
            color: #6b7280;
        }
        .report-title {
            font-size: 14px;
            font-weight: bold;
            color: #111827;
            margin-top: 6px;
        }

        /* ======= SECCIÓN DATOS GENERALES ======= */
        .section {
            margin-bottom: 10px;
        }
        .section-title {
            font-size: 12px;
            font-weight: bold;
            color: #374151;
            margin-bottom: 4px;
        }
        .info-row {
            width: 100%;
        }
        .info-row td {
            padding: 2px 0;
            vertical-align: top;
        }
        .info-label {
            font-weight: bold;
            color: #4b5563;
            width: 90px;
        }

        /* ======= RESUMEN (CARDS) ======= */
        .summary {
            margin-top: 4px;
            margin-bottom: 10px;
        }
        .summary-table {
            width: 100%;
        }
        .summary-card {
            border-radius: 6px;
            padding: 6px 8px;
            border: 1px solid #e5e7eb;
        }
        .summary-header {
            font-size: 11px;
            font-weight: bold;
            margin-bottom: 2px;
        }
        .summary-work {
            background-color: #ecfdf5;  /* verde muy claro */
            border-color: #6ee7b7;
        }
        .summary-break {
            background-color: #fff7ed;  /* naranja muy claro */
            border-color: #fed7aa;
        }
        .summary-total {
            background-color: #eff6ff;  /* azul claro */
            border-color: #bfdbfe;
        }
        .summary-value {
            font-size: 13px;
            font-weight: bold;
        }
        .summary-note {
            font-size: 10px;
            color: #6b7280;
        }

        /* ======= TABLA DE EVENTOS ======= */
        table.events {
            width: 100%;
            border-collapse: collapse;
            margin-top: 4px;
        }
        table.events th,
        table.events td {
            border: 1px solid #e5e7eb;
            padding: 4px 5px;
        }
        table.events th {
            background-color: #f3f4f6;
            font-size: 11px;
            text-align: left;
        }
        table.events td {
            font-size: 10px;
        }

        .text-center { text-align: center; }
        .text-right  { text-align: right;  }
        .text-muted  { color: #6b7280;     }

        /* ======= BADGES / COLORES ESTADO ======= */
        .badge {
            display: inline-block;
            padding: 1px 5px;
            border-radius: 999px;
            font-size: 9px;
            font-weight: bold;
        }
        .badge-work {
            background-color: #bbf7d0;  /* verde suave */
            color: #064e3b;
        }
        .badge-break {
            background-color: #fed7aa;  /* naranja suave */
            color: #7c2d12;
        }
        .badge-other {
            background-color: #e5e7eb;  /* gris */
            color: #374151;
        }

        .small {
            font-size: 9px;
            color: #6b7280;
        }

        /* ======= PIE DE PÁGINA ======= */
        .footer {
            margin-top: 10px;
            padding-top: 6px;
            border-top: 1px solid #e5e7eb;
            font-size: 9px;
            color: #9ca3af;
        }
